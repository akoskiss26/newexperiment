
@model List<Holidaysheet.Models.Holiday>
@{
    ViewBag.Title = "Holiday";
}

<h2>Holiday</h2>

<ul>
    @foreach (var item in Model)
    {
        <li>@item.Name </li>
    }



</ul>
<h1> @Model[1].Name </h1>

<table style="width:50%">
    <tr>
        <th>név</th>
        <th>szabadság</th>
    </tr>

    @foreach (var item in Model)
    {
    <tr>
        <td>@item.Name</td>      
        <td>@item.Days</td>
    </tr> 
    }

    
    
</table>

